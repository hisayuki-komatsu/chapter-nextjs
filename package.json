{
  "name": "chapter-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -p 3000",
    "dev:path": "npm run gen:path --watch",
    "build": "gen:path && next build && next export",
    "prepare": "husky install && npm run gen:api && run-s gen:*",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "next lint --dir src --fix",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watch",
    "chromatic": "chromatic --exit-zero-on-changes",
    "gen:api": "rimraf src/apis/Autogen && openapi2aspida",
    "gen:yml2json": "yaml-convert --input openapi.yml --output openapi.json",
    "gen:examples": "node ./src/apis/generateExamples.js && prettier --write ./src/apis/Autogen/examples.ts",
    "gen:types": "node ./src/apis/generateTypes.js && prettier --write ./src/apis/Autogen/@types/index.ts",
    "gen:path": "pathpida -p .gitignore -o src/libs",
    "gen:ui": "hygen generators ui"
  },
  "engines": {
    "node": "18.13.0"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@aspida/swr": "^1.11.0",
    "@next/font": "13.0.7",
    "axios": "^0.27.2",
    "camelize-ts": "^2.3.0",
    "envsafe": "^2.0.3",
    "next": "13.0.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.13.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/styled-components": "^5.1.26",
    "babel-loader": "^8.3.0",
    "chromatic": "^6.15.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "^13.2.3",
    "eslint-plugin-storybook": "^0.6.10",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.4.0",
    "jest-environment-jsdom": "^29.4.0",
    "lint-staged": "^13.1.2",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "openapi2aspida": "^0.21.0",
    "pathpida": "^0.20.1",
    "rimraf": "^4.3.0",
    "storybook-addon-next": "^1.7.1",
    "styled-components": "^5.3.6",
    "typescript": "^4.9.5",
    "yaml-convert": "^1.0.1"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
